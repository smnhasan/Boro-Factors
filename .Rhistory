library(readxl)
library(dplyr)
library(Matrix)
library(lme4)
library(lmerTest)
setwd("E:\\Niaz Bhai\\Boro")
borodata <- read.csv("data_final.csv")
borodata$Y <- as.numeric(as.character(borodata$Y))
mod1 <- lmer(Yield ~ r.i + t.k + MAXT + MINT + RAIN + HUMIDITY + SUNSH +Wind+
(1|G) + (1|L) + (1|Y) + (1|L:Y) + (1|L:G) +(1|G:Y)+(1|G:L:Y),borodata,
control=lmerControl(check.nobs.vs.nlev = "ignore",
check.nobs.vs.rankZ = "ignore",
check.nobs.vs.nRE="ignore"))
summary(mod1)
library(cAIC4)
library(MASS)
stepcAIC(mod1,direction="backward",
data=borodata,trace=TRUE)
step(mod1)
(step_res <- step(mod1))
final <- get_model(step_res)
anova(final)
confint(mod1)
confint(anova(final))
confint(final)
